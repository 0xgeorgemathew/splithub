{
  "$schema": "https://schema.railpack.com",
  "steps": {
    "install": {
      "cache": [
        "node_modules",
        ".yarn/cache"
      ],
      "commands": [
        "node .yarn/releases/yarn-3.2.3.cjs install --immutable"
      ]
    },
    "build": {
      "inputs": [
        {
          "step": "install"
        }
      ],
      "cache": [
        "packages/nextjs/.next/cache"
      ],
      "env": {
        "NODE_ENV": "production",
        "NEXT_TELEMETRY_DISABLED": "1",
        "SKIP_ENV_VALIDATION": "true"
      },
      "commands": [
        "node .yarn/releases/yarn-3.2.3.cjs workspace @se-2/nextjs build"
      ]
    }
  },
  "deploy": {
    "startCommand": "node .yarn/releases/yarn-3.2.3.cjs workspace @se-2/nextjs serve"
  }
}